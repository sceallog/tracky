<%= form_with(model: @project, local: true) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :start_date %>
  <%= f.date_field :start_date, class: 'form-control' %>

  <%= f.label :target_end_date %>
  <%= f.date_field :target_end_date, class: 'form-control' %>

  <%= f.submit %>

<% end %>
